function [H,We,Wd]=compute(X,blta,k,increSize,firstSize)
[U,S,V]=svd(X(:,1:firstSize),'econ');
for i=1:(size(X,1)-firstSize)/increSize
    [U,S,V]=svdUpdate(U,S,V,X(:,firstSize:firstSize+i*increSize));
end
[m,n]=size(X);
p=floor(blta*m);
We=V(1:p,:)*inv(S)*U';
H=sigmf(We*X,[1,0]);
Hni=H'*inv(H*H'+k*eye(size(H*H')))
Wd=X*Hni;
We=Wd';
H=sigmf(We*X,[1,0]);
Wd=X*H'*inv(H*H'+k*eye(size(H*H')));
end




